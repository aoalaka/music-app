<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.qplayer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/soundcloud"
        android:label="@string/app_name"
        android:roundIcon="@drawable/soundcloud_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ArtistsActivity"
            android:label="Artists" />
        <activity
            android:name=".AlbumsActivity"
            android:label="Albums" />
        <activity
            android:name=".SongsActivity"
            android:label="Songs" />
        <activity
            android:name=".PlaylistsActivity"
            android:label="Playlists"></activity>
        <activity android:name=".NowPlayingActivity"></activity>
    </application>

</manifest>